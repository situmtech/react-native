{
  "name": "@situm/react-native",
  "version": "3.14.22",
  "description": "Set of utilities that allow any developer to build React Native location based apps using Situm's indoor positioning system.",
  "private": false,
  "repository": "https://github.com/situmtech/react-native",
  "author": "Situm Technologies <mobile@situm.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/situmtech/react-native/issues"
  },
  "homepage": "https://github.com/situmtech/react-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "README.md",
    "LICENSE",
    "SECURITY-POLICY.md",
    "security.txt",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "yarn typescript && yarn lint && yarn test:jest",
    "test:jest": "jest",
    "typescript": "tsc --noEmit",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "bob build; yarn doc",
    "release": "release-it",
    "example": "yarn --cwd example",
    "build:android": "cd example/android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd example/ios && xcodebuild -workspace AwesomeLibraryExample.xcworkspace -scheme AwesomeLibraryExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO",
    "bootstrap": "yarn example && yarn && yarn example pods",
    "doc": "typedoc --out ./docs/types ./src  --tsconfig ./tsconfig.json && mkdir -p ./docs/types/docs/assets && cp -r ./docs/assets/* ./docs/types/docs/assets/",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build"
  },
  "keywords": [
    "react-native",
    "situm",
    "indoor positioning",
    "wayfinding",
    "ios",
    "android"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@commitlint/config-conventional": "^17.6.7",
    "@evilmartians/lefthook": "^1.2.20.7.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.7",
    "@types/react-native": "0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitlint": "^17.6.6",
    "del-cli": "^5.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.5",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-native": "^0.72.12",
    "react-native-builder-bob": "^0.21.3",
    "react-test-renderer": "18.2.0",
    "release-it": "^16.1.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react-dom": "^18.2.0",
    "react-native-webview": "*"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "yarn@1.22.15",
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/",
      "<rootDir>/example/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {},
  "sdkVersions": {
    "android": "3.30.0@aar",
    "ios": "3.32.0"
  }
}
