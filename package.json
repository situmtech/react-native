{
  "name": "@situm/react-native",
  "version": "3.0.1",
  "description": "Set of utilities that allow any developer to build React Native location based apps using Situm's indoor positioning system.",
  "private": false,
  "repository": "https://github.com/situmtech/react-native",
  "author": "Situm Technologies <mobile@situm.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/situmtech/react-native/issues"
  },
  "homepage": "https://github.com/situmtech/react-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "ReactNativeSitumPlugin.podspec",
    "README.md",
    "LICENSE",
    "SECURITY-POLICY.md",
    "security.txt",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "yarn typescript && yarn lint && yarn test:jest",
    "test:jest": "jest",
    "typescript": "tsc --noEmit",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "bob build",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn && yarn example pods",
    "doc": "typedoc --out ./docs/types ./src  --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "react-native",
    "situm",
    "indoor positioning",
    "wayfinding",
    "ios",
    "android"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.2",
    "@types/react-native": "0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react": "^18.2.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitlint": "^17.6.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.8.8",
    "react-native-builder-bob": "^0.20.4",
    "react-native": "^0.72.1",
    "react-test-renderer": "18.2.0",
    "react": "^18.2.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react-dom": "^18.2.0",
    "react-native-permissions": "*",
    "react-native-webview": "*"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/",
      "<rootDir>/example/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "sdkVersions": {
    "android": "3.1.0@aar",
    "ios": "2.61.1"
  }
}
