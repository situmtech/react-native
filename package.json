{
  "name": "react-native-situm-plugin",
  "description": "React Native wrapper around Situm SDK",
  "homepage": "https://github.com/situmtech/situm-react-native-plugin#readme",
  "repository": "https://github.com/situmtech/situm-react-native-plugin",
  "bugs": {
    "url": "https://github.com/situmtech/situm-react-native-plugin/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "version": "0.0.14",
  "private": false,
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-wayfinding.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.20.7",
    "@babel/runtime": "^7.20.13",
    "@commitlint/config-conventional": "^17.4.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-native": "^0.71.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.4.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitlint": "^17.4.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.4.1",
    "metro-react-native-babel-preset": "^0.74.1",
    "npm-dts": "^1.3.12",
    "prettier": "^2.8.3",
    "react": "^16.11.0",
    "react-native": "0.71.1",
    "react-native-builder-bob": "^0.20.3",
    "react-test-renderer": "16.11.0",
    "release-it": "^15.6.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {},
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "scripts": {
    "android": "react-native run-android",
    "clean": "rm -r lib node_modules",
    "ios": "react-native run-ios",
    "lint:fix": "eslint --fix \"**/*.{js,ts,tsx}\"",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "prepare": "bob build",
    "release": "release-it",
    "start": "react-native start",
    "test:jest": "jest /src --no-watchman",
    "test": "yarn lint && yarn test:jest"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "situm"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": false,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": false,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "sdkVersions": {
    "android": "2.83.5@aar",
    "ios": "2.52.1"
  }
}
